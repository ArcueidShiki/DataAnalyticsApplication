{
  "name": "frontend",
  "version": "1.0.0",
  "description": "## Configuration",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --check",
    "lint:staged": "lint-staged",
    "precommit": "lint-staged",
    "prepush": "npm run lint:check",
    "prettier": "prettier --write .",
    "prepare": "cd .. && husky frontend/.husky",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "lint-staged": "^15.5.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
